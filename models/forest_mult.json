{
  "mode": "multiclass",
  "model": "forest",
  "params": {
    "n_estimators": 500,
    "max_depth": null,
    "min_samples_split": 5,
    "min_samples_leaf": 1,
    "max_features": "log2"
  },
  "dataset": "dataset.csv",
