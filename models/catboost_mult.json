{
  "mode": "multiclass",
  "model": "catboost",
  "params": {
    "iterations": 1300,
    "learning_rate": 0.1,
    "depth": 7,
    "l2_leaf_reg": 5,
    "bagging_temperature": 0.5,
    "random_strength": 7,
    "verbose": 0,
    "allow_writing_files": false,
    "random_state": 42
  },
  "dataset": "dataset.csv",
  "log": true,
  "combinations": [{
    "name": "user_rtXid",
    "c1": "user_rt",
    "c2": "user_id",
    "op": "mult"
  },{
    "name": "hashtags2rt",
    "c1": "num_hashtags",
    "c2": "num_rt",
    "op": "div"
  }],
  "num_exclude": ["polarity"],
  "count_matrix": true,
  "scale": false,
  "remove_corr": false,
  "score_users": false,
  "top_features": 0
}
